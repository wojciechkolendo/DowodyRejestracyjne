<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_margin="16dp"
	android:orientation="vertical"
	tools:ignore="RtlHardcoded,DisableBaselineAlignment">

	<com.google.android.material.textfield.TextInputLayout
		style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<com.google.android.material.textfield.TextInputEditText
			android:id="@+id/organWydajacyTextView"
			style="@style/Base.DowodyRejestracyjne.Widget.TextInputEditText"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:hint="@string/result_page1_organ_wydajacy"/>

	</com.google.android.material.textfield.TextInputLayout>

	<com.google.android.material.textfield.TextInputLayout
		style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="16dp">

		<com.google.android.material.textfield.TextInputEditText
			android:id="@+id/numerRejestracyjnyTextView"
			style="@style/Base.DowodyRejestracyjne.Widget.TextInputEditText"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:hint="@string/result_page1_a"/>

	</com.google.android.material.textfield.TextInputLayout>

	<com.google.android.material.textfield.TextInputLayout
		style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="16dp"
		app:layout_constraintLeft_toLeftOf="parent">

		<com.google.android.material.textfield.TextInputEditText
			android:id="@+id/danePojazduTextView"
			style="@style/Base.DowodyRejestracyjne.Widget.TextInputEditText"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:hint="@string/result_page1_d"/>

	</com.google.android.material.textfield.TextInputLayout>

	<com.google.android.material.textfield.TextInputLayout
		style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="16dp">

		<com.google.android.material.textfield.TextInputEditText
			android:id="@+id/vinTextView"
			style="@style/Base.DowodyRejestracyjne.Widget.TextInputEditText"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:hint="@string/result_page1_e"/>

	</com.google.android.material.textfield.TextInputLayout>

	<com.google.android.material.textfield.TextInputLayout
		style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="16dp">

		<com.google.android.material.textfield.TextInputEditText
			android:id="@+id/dataBTextView"
			style="@style/Base.DowodyRejestracyjne.Widget.TextInputEditText"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:hint="@string/result_page1_b"/>

	</com.google.android.material.textfield.TextInputLayout>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="16dp"
		android:orientation="horizontal"
		android:weightSum="2">

		<com.google.android.material.textfield.TextInputLayout
			style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginRight="6dp"
			android:layout_weight="1">

			<com.google.android.material.textfield.TextInputEditText
				android:id="@+id/dataITextView"
				style="@style/Base.DowodyRejestracyjne.Widget.TextInputEditText"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:hint="@string/result_page1_i"
				android:maxLines="1"/>

		</com.google.android.material.textfield.TextInputLayout>

		<com.google.android.material.textfield.TextInputLayout
			style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="6dp"
			android:layout_weight="1">

			<com.google.android.material.textfield.TextInputEditText
				android:id="@+id/dataHTextView"
				style="@style/Base.DowodyRejestracyjne.Widget.TextInputEditText"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:hint="@string/result_page1_h"
				android:maxLines="1"/>

		</com.google.android.material.textfield.TextInputLayout>

	</LinearLayout>

</LinearLayout>